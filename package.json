    "@eslint/markdown": "^7.4.0",
    "@html-eslint/eslint-plugin": "^0.47.0",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@kazupon/prettier-config": "^0.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.18.9",
    "@vitest/eslint-plugin": "^1.3.16",
