{
  "name": "@kazupon/eslint-config",
  "description": "ESLint config for @kazupon",
  "version": "0.23.0",
  "author": {
    "email": "kawakazu80@gmail.com",
    "name": "kazuya kawaguchi"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/eslint-config.git"
  },
  "keywords": [
    "config",
    "eslint"
  ],
  "homepage": "https://github.com/kazupon/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": ">= 20"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "changelog": "gh-changelogen --repo=kazupon/eslint-config",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "fix": "pnpm run --parallel --color \"/^fix:/\"",
    "fix:eslint": "eslint . --fix",
    "fix:knip": "knip --fix --no-exit-code",
    "fix:prettier": "prettier . --write",
    "lint": "pnpm run --parallel --color \"/^lint:/\"",
    "lint:eslint": "eslint .",
    "lint:knip": "knip",
    "lint:prettier": "prettier . --check",
    "prepack": "pnpm typegen && pnpm build",
    "prepare": "git config --local core.hooksPath .githooks",
    "release": "bumpp --commit \"release: v%s\" --all --push --tag",
    "test": "vitest run --typecheck",
    "typecheck": "tsc -p .",
    "typegen": "pnpx jiti scripts/typegen.ts"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.22.0",
    "@kazupon/jts-utils": "^0.6.0",
    "eslint-flat-config-utils": "^2.0.1",
    "globals": "^16.0.0"
  },
  "peerDependencies": {
    "@eslint/css": ">=0.4.0",
    "@eslint/markdown": ">=6.1.0",
    "@vitest/eslint-plugin": ">=1.0.0",
    "eslint": ">=8.56.0 || >=9.0.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-import-resolver-typescript": ">=3.6.0",
    "eslint-plugin-import": ">=2.31.0",
    "eslint-plugin-jsdoc": ">=48.5.0",
    "eslint-plugin-jsonc": ">=2.16.0",
    "eslint-plugin-promise": ">=6.4.0",
    "eslint-plugin-react": ">=7.35.0",
    "eslint-plugin-react-hooks": ">=5.2.0",
    "eslint-plugin-react-refresh": ">=0.4.19",
    "eslint-plugin-regexp": ">=2.6.0",
    "eslint-plugin-svelte": ">=2.43.0",
    "eslint-plugin-toml": ">=0.11.0",
    "eslint-plugin-unicorn": ">=57.0.0",
    "eslint-plugin-unused-imports": ">=4.1.0",
    "eslint-plugin-vue": ">=9.24.0",
    "eslint-plugin-vue-composable": ">=1.0.0",
    "eslint-plugin-vue-scoped-css": ">=2.8.0",
    "eslint-plugin-vuejs-accessibility": ">=2.4.0",
    "eslint-plugin-yml": ">=1.14.0",
    "svelte": ">=4.0.0",
    "typescript-eslint": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "@eslint/css": {
      "optional": true
    },
    "@eslint/markdown": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-jsdoc": {
      "optional": true
    },
    "eslint-plugin-jsonc": {
      "optional": true
    },
    "eslint-plugin-promise": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-regexp": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-toml": {
      "optional": true
    },
    "eslint-plugin-unicorn": {
      "optional": true
    },
    "eslint-plugin-unused-imports": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vue-composable": {
      "optional": true
    },
    "eslint-plugin-vue-scoped-css": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-plugin-yml": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/css": "^0.5.0",
    "@eslint/markdown": "^6.3.0",
    "@kazupon/prettier-config": "^0.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.11",
    "@vitest/eslint-plugin": "^1.1.38",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.8",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-plugin-vue-composable": "^1.0.0",
    "eslint-plugin-vue-scoped-css": "^2.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-typegen": "^2.1.0",
    "gh-changelogen": "^0.2.8",
    "jiti": "^2.4.2",
    "knip": "^5.46.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "svelte": "^5.0.0",
    "tsdown": "^0.6.9",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "packageExtensions": {
      "@eslint/css": {
        "peerDependencies": {
          "@types/css-tree": "2.3.10"
        }
      }
    }
  },
  "prettier": "@kazupon/prettier-config",
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.{js,mjs,cjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,jsonc,json5,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
