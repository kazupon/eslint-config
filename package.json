{
  "name": "@kazupon/eslint-config",
  "description": "ESLint config for @kazupon",
  "version": "0.14.0",
  "author": {
    "email": "kawakazu80@gmail.com",
    "name": "kazuya kawaguchi"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/eslint-config.git"
  },
  "keywords": [
    "config",
    "eslint"
  ],
  "homepage": "https://github.com/kazupon/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.8.0",
  "engines": {
    "node": ">= 18.18"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "changelog": "gh-changelogen --repo=kazupon/eslint-config",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "fix": "run-p \"fix:* {@}\" --",
    "fix:eslint": "eslint --flag unstable_ts_config . --fix",
    "fix:prettier": "prettier . --write",
    "lint": "run-p \"lint:* {@}\" --",
    "lint:eslint": "eslint --flag unstable_ts_config .",
    "lint:prettier": "prettier . --check",
    "prepack": "pnpm typegen && pnpm build",
    "prepare": "git config --local core.hooksPath .githooks",
    "release": "bumpp --commit \"release: v%s\" --all --push --tag",
    "test": "vitest",
    "typecheck": "tsc -p .",
    "typegen": "pnpx jiti scripts/typegen.ts"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@eslint/js": "^9.0.0",
    "eslint-flat-config-utils": "^0.3.0",
    "globals": "^15.8.0"
  },
  "peerDependencies": {
    "eslint": ">=8.56.0 || >=9.0.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-plugin-jsdoc": ">=48.5.0",
    "eslint-plugin-jsonc": ">=2.16.0",
    "eslint-plugin-promise": ">=6.4.0",
    "eslint-plugin-react": ">=7.35.0",
    "eslint-plugin-react-hooks": ">=4.6.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "eslint-plugin-regexp": ">=2.6.0",
    "eslint-plugin-svelte": ">=2.43.0",
    "eslint-plugin-toml": ">=0.11.0",
    "eslint-plugin-unicorn": ">=54.0.0",
    "eslint-plugin-vue": ">=9.24.0",
    "eslint-plugin-yml": ">=1.14.0",
    "svelte": ">=4.0.0",
    "typescript-eslint": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-jsdoc": {
      "optional": true
    },
    "eslint-plugin-jsonc": {
      "optional": true
    },
    "eslint-plugin-promise": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-regexp": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-toml": {
      "optional": true
    },
    "eslint-plugin-unicorn": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-yml": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@kazupon/prettier-config": "^0.1.1",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.5.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-svelte": "^2.43.0",
    "eslint-plugin-toml": "^0.11.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-typegen": "^0.3.1",
    "gh-changelogen": "^0.2.8",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.2",
    "svelte": "^4.2.18",
    "tsdown": "^0.2.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vitest": "^2.0.0"
  },
  "prettier": "@kazupon/prettier-config",
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --flag unstable_ts_config --fix"
    ],
    "*.{js,mjs,cjs}": [
      "prettier --write",
      "eslint --flag unstable_ts_config --fix"
    ],
    "*.{json,jsonc,json5,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
